<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">メニュー</h3>
  </div>
  <div class="panel-body">
    <div class="page-header">
      <h1>筋トレメニュー　一覧</h1>
    </div>

    <div class="table-responsive">
      <table class="table table-striped table-bordered table-hover">
        <thead>
          <tr>
            <th></th>
            <th>バーベル</th>
            <th>ダンベル</th>
            <th>マシン</th>
            <th>自重</th>
          </tr>
        </thead>

        <tbody>
          <%= content_tag_for(:tr, @muscles) do |muscle| %>
          <td><%= muscle.part %></td>
          <td><%= muscle.events.where(event_class:"バーベル").event_name %></td>
          <td><%=  %></td>
          <td><%=  %></td>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
