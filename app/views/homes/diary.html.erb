<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">筋トレ日記</h3>
  </div>
  <div class="panel-body">
    <h1><%= @date%></h1>
    <h2>筋トレ記録</h2>
    <table>
      <% @diaries.each do |diary| %>
      <tr>
        <td><%= diary.weight %></td>
        <td><%= diary.num %></td>
        <td><%= diary.set_num %></td>
        <td><%= diary.date %></td>
        <td><%= diary.note %></td>
      </tr>
      <% end %>
    </table>
    <h2>プロフィール</h2>
  </div>
</div>
